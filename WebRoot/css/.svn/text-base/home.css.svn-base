*{padding:0; margin:0; list-style:none}

body{background:#f0f0f2; font-family: "Microsoft Yahei";}
div#image{margin-top: 100px;}
div#image img{width: 150px; height: 150px; margin:0px auto; display: block;}
h2{text-align: center;}
table#table1{margin: 10px auto; border-spacing: 0px;}
table#table1 td{text-align: center; width: 100px; line-height: 50px; border-bottom: 1px solid #bababc;}
div#profile{width: 300px; margin:10px auto;}
div#vertical_line{width:1px; height:100%; background-color: #bababc; float:left;}
div#profile_and_tag{float:left; }
div#tag img{width: 1.5em; height: 1.5em;}
em{font-style: normal;}
div#profile h3{padding-left: 10px;}